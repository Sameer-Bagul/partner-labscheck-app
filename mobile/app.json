{
  "expo": {
    "name": "LabsCheck Mobile",
    "slug": "labscheck-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#7B2CBF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.labscheck.partner"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#7B2CBF"
      },
      "package": "com.labscheck.partner",
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY"
        }
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-secure-store"
      ],
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
